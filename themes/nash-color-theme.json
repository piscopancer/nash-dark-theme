  "tokenColors": [
    {
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.italic", "entity.other.attribute-name"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.bold markup.italic"],
      "settings": {
        "fontStyle": "italic bold"
      }
    },
    {
      "scope": ["constant.character.escape", "punctuation", "meta.brace", "storage.type", "storage.modifier", "keyword.control", "keyword.operator", "comment", "punctuation.definition.comment", "meta.separator"],
      "settings": {
        "foreground": "#515559"
      }
    },
    {
      "scope": ["support.type", "keyword.control.at-rule", "entity.name.function", "variable"],
      "settings": {
        "foreground": "#b6b7b8"
      }
    },
    {
      "scope": ["string", "string.regexp", "constant.other.symbol"],
      "settings": {
        "foreground": "#8ca66d"
      }
    },
    {
      "scope": ["constant", "markup.inline.raw"],
      "settings": {
        "foreground": "#cd8e52"
      }
    },
    {
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#ca614f"
      }
    },
    {
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#cd7763"
      }
    },
    {
      "scope": ["punctuation.definition.tag"],
      "settings": {
        "foreground": "#623d36"
      }
    },
    {
      "name": "Info Tokens",
      "scope": "token.info-token",
      "settings": {
        "foreground": "#73beff"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#d2bc7a"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#ca614f"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#A087D4"
      }
    },

    // Chinese Roofs

    {
      "scope": ["translation.text.chr"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#515559"
      }
    },
    {
      "scope": ["translation.vocabulary.word.original.chr"],
      "settings": {}
    },
    {
      "scope": ["translation.vocabulary.word.pinyin.chr"],
      "settings": {
        "foreground": "#515559"
      }
    },
    {
      "scope": "translation.vocabulary.word.pinyin.char.chr",
      "settings": {
        "foreground": "#8ca66d"
      }
    },
    {
      "scope": "translation.vocabulary.word.pinyin.tone.chr",
      "settings": {
        "foreground": "#515559"
      }
    },
    {
      "scope": "translation.vocabulary.word.translation.chr",
      "settings": {
        "fontStyle": "italic"
      }
    }
  ]
}
